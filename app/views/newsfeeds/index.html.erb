

 <%= stylesheet_link_tag "newsfeeds" %>

 <div class="container">
   <div class="row">
     <div class="col-lg-12 newsfeed">
       <h1 class="title-text">NSCS Announcements and News</h1>
         <table class="table">
           <thead class="thead-dark">
             <tr>
               <th scope="col">Announcement</th>
             </tr>
           </thead>
           <% @newsfeeds.each do |news| %>
             <tbody>
               <tr>
                 <td><%= news.news %> </td>
                 <td><%= link_to "Edit", edit_newsfeed_path(news) %></td>
                 <td><%= link_to "Delete", news, method: :delete, data: {confirm: "Are you sure?"} %></td>
               </tr>
             </tbody>
           <% end %>
         </table>
         <%= link_to "Post New Announcement", newsfeeds_new_path, class:"btn btn-primary" %>
     </div>
   </div>
 </div>
